{
    "name": "livestream",
    "version": "1.10.6",
    "private": true,
    "description": "livestream",
    "author": "Hoeu Sophanorin",
    "license": "MIT",
    "main": "server.js",
    "scripts": {
        "build": "mkdir -p dist && find dist/* -maxdepth 0 ! -name public -exec rm -rf {} \\; && tsc && ln -s ../certs dist/certs && chmod 755 dist/server.js && ( for fileExt in yaml json toml ; do [ -f config/config.$fileExt ] && cp config/config.$fileExt dist/config/; done ) | true && touch 'dist/ __AUTO_GENERATED_CONTENT_REFRESHED_AFTER_REBUILDING!__ '",
        "start": "node server.js",
        "start:dev": "nodemon server.js",
        "connect": "node connect.js"
    },
    "dependencies": {
        "@socket.io/admin-ui": "^0.2.0",
        "@socket.io/redis-adapter": "^7.0.0",
        "awaitqueue": "^2.3.3",
        "aws-sdk": "^2.1000.0",
        "axios": "^0.22.0",
        "base-64": "^1.0.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "debug": "^4.3.2",
        "dotenv": "^10.0.0",
        "eloquent-ffmpeg": "^0.14.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "express-socket.io-session": "^1.3.5",
        "fluent-ffmpeg": "^2.1.2",
        "fluent-ffmpeg-util": "^0.2.0-alpha.1",
        "helmet": "^4.6.0",
        "ims-lti": "^3.0.2",
        "jsonwebtoken": "^8.5.1",
        "lodash.throttle": "^4.1.1",
        "mediasoup": "^3.10.0",
        "mongoose": "^6.0.9",
        "openid-client": "^4.9.0",
        "passport": "^0.5.0",
        "passport-local": "^1.0.0",
        "passport-lti": "0.0.7",
        "passport-saml": "^3.2.0",
        "pidusage": "^2.0.21",
        "prom-client": ">=14.0.0",
        "redis": "^3.1.2",
        "s3-upload-stream": "^1.0.7",
        "socket.io": "^4.2.0",
        "spdy": "^4.0.2",
        "uuid": "^8.3.2"
